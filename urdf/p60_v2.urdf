<?xml version="1.0"?>
<robot name="p60_v2">
  <material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <!-- Base Link -->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.5 0.5 0.25"/>
      </geometry>
      <origin xyz="0 0 0.06" rpy="0 0 0" />
      <material name="white"/>
    </visual>
   <!-- Base collision -->
   <collision>
     <geometry>
       <box size="0.5 0.5 0.25"/>
     </geometry>
      <origin xyz="0 0 0.06" rpy="0 0 0" />
   </collision>
  </link>

  <!-- Right Wheel -->
  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <material name="blue"/>
    </visual>
    <!-- Right Wheel collision -->
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </collision>
   </link>
    <joint name="joint_right_wheel" type="continuous">
     <parent link="base_link"/>
     <child link="right_wheel"/>
     <origin xyz="0 -0.30 0" rpy="0 0 0" />
     <axis xyz="0 1 0" />
    </joint>

  <!-- Left Wheel -->
  <link name="left_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="blue"/>
    </visual>
    <!-- Left Wheel collision -->
    <collision>
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </collision>
  </link>
  <joint name="joint_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0 0.30 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>
 
  <!-- Front Wheel Steer-->
  <link name="front_wheel_steer">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </visual>
  </link>
  <joint name="joint_front_wheel_steer" type="revolute">
    <parent link="base_link"/>
    <child link="front_wheel_steer"/>
    <origin xyz="0.30 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5" />
  </joint>
  <!-- Front Wheel -->
  <link name="front_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="blue"/>
    </visual>
    <!-- Front Wheel collision -->
    <collision>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </collision>
  </link>
  <joint name="joint_front_wheel" type="continuous">
    <parent link="front_wheel_steer"/>
    <child link="front_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <!-- Back Wheel Steer-->
  <link name="back_wheel_steer">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </visual>
  </link>
  <joint name="joint_back_wheel_steer" type="revolute">
    <parent link="base_link"/>
    <child link="back_wheel_steer"/>
    <origin xyz="-0.30 0 0" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <mimic joint="joint_front_wheel_steer"
           multiplier="-1"
           offset="0"/>
    <limit effort="1000.0" lower="-1.0" upper="1.0" velocity="0.5" />
  </joint>
  <!-- Back Wheel -->
  <link name="back_wheel">
    <visual>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
      <material name="blue"/>
    </visual>
    <!-- Back Wheel collision -->
    <collision>
      <origin xyz="0 0 0" rpy="1.570795 0 0" />
      <geometry>
        <cylinder length="0.1" radius="0.2" />
      </geometry>
    </collision>
  </link>
  <joint name="joint_back_wheel" type="continuous">
    <parent link="back_wheel_steer"/>
    <child link="back_wheel"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

</robot>
